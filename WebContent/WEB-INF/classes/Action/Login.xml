<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation"	value="false" />
	<constant name="struts.devMode" value="false" />
	<constant name="struts.custom.i18n.resources" value="resources/Message"></constant>

	<package name="Login" extends="struts-default" namespace="/" >	
		
		<action name="loginAction"	class="Business.Login" method="signIn">
			
			<result name="success">Semester.jsp</result>
			<result name="error">Login.jsp</result>
			<result name="studentalreadylogin">Semester.jsp</result>
			<result name="parentalreadylogin">Login.jsp</result>
			<result name="facultyalreadylogin">Login.jsp</result>
			
		</action>
		
		
		
	</package>
	
	
</struts>
