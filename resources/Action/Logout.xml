<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation"	value="false" />
	<constant name="struts.devMode" value="false" />
	<constant name="struts.custom.i18n.resources" value="resources/Message"></constant>

	<package name="Logout" extends="struts-default" namespace="/" >	
		
		<action name="logout"	class="Business.Logout" method="logout">
			
			<result name="success">WEB-INF/JSPView/LoginView/Login.jsp</result>
			
			
		</action>
		
		
		<action name="logoutAdmin"	class="Business.Logout" method="logoutAdmin">
			
			<result name="success">WEB-INF/JSPView/LoginView/Login.jsp</result>
			
			
		</action>
		
		
		
	</package>
	
	
</struts>
