<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<constant name="struts.enable.DynamicMethodInvocation"	value="false" />
	<constant name="struts.devMode" value="false" />
	<constant name="struts.custom.i18n.resources" value="resources/Message"></constant>

	<package name="xyz" extends="struts-default" namespace="/" >	
	
	   <action name="facultySelectAction"	class="Business.FacultySelection" method="getOptionSelected">
			<result name="create">WEB-INF/JSPView/FacultyView/StudentOrSubject.jsp</result>
			<result name="view">WEB-INF/JSPView/FacultyView/StudentOrSubject.jsp</result>
			<result name="delete">WEB-INF/JSPView/FacultyView/StudentOrSubject.jsp</result>
			<result name="edit">WEB-INF/JSPView/FacultyView/StudentOrSubject.jsp</result>
			<result name="error">WEB-INF/JSPView/LoginView/Login.jsp</result>

		</action>
		
		<action name="wxyz"	class="Business.StudentOrSubject" method="sf1">
			<result name="studentoption">WEB-INF/JSPView/FacultyView/SelectStudent.jsp</result>
			<result name="error">WEB-INF/JSPView/LoginView/Login.jsp</result>

		</action>
		
		<action name="vwxyz" class="Business.SelectStudent" method="sf2">
			<result name="updatemarks">WEB-INF/JSPView/FacultyView/EditMarks.jsp</result>
			<result name="deletemarks">WEB-INF/JSPView/FacultyView/DeleteMarks.jsp</result>
			<result name="createmarks">WEB-INF/JSPView/FacultyView/EditMarks.jsp</result>
			<result name="error">WEB-INF/JSPView/LoginView/Login.jsp</result>

		</action>
		
		<action name="uvwxyz" class="Business.EditMarks" method="sf3">
			<result name="error">WEB-INF/JSPView/FacultyView/new.jsp</result>
		</action>
		
		<action name="deleteStudentMarks" class="Business.DeleteMarks" method="deleteStudentMarks">
			<result name="error">WEB-INF/JSPView/FacultyView/new.jsp</result>
		</action>
		
	</package>
	
	
</struts>
